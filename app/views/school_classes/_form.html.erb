<h1>School Class Form</h1>
<%= form_for @school_class do |f| %>
  <%= label_tag :'school_class[title]', "Title" %><br>
  <%= text_field_tag :'school_class[title]' %><br>

  <%= label_tag :'school_class[room_number]', "Room Number" %><br>
  <%= text_field_tag :'school_class[room_number]' %><br>

  <%= f.submit %>
<% end %>
